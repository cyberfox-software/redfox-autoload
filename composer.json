{
  "name": "cyberfox/redfox-autoload",
  "description": "The autoload component provides a facility to automatic load your classes without include your class files.",
  "type": "library",
  "minimum-stability": "dev",
  "keywords": ["red fox", "library", "autoload", "class load"],
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Graf",
      "email": "christian.graf@cyberfox.at"
    },
    {
      "name": "Cyberfox Software Solutions e.U.",
      "email": "office@cyberfox.at",
      "homepage": "http://cyberfox.at"
    }
  ],
  "require": {
    "php": ">= 5.5.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8|^5.5"
  },
  "autoload": {
    "psr-4": {
      "Redfox\\Autoload\\": "src",
      "Redfox\\Autoload\\UnitTest\\": "test/unit/"
    },
    "exclude-from-classmap": [
      "/test/"
    ]
  }
}