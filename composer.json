{
  "name": "cyberfox/redfox-autoload",
  "description": "The autoload component provides a facility to automatic load your classes without include your class files.",
  "type": "library",
  "minimum-stability": "dev",
  "keywords": ["red fox", "library", "autoload", "class load"],
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Graf",
      "email": "christian.graf@cyberfox.at"
    },
    {
      "name": "Cyberfox Software Solutions e.U.",
      "email": "office@cyberfox.at",
      "homepage": "http://cyberfox.at"
    }
  ],
  "require": {
    "php": ">= 5.5.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3",
    "pdepend/pdepend" : "@stable",
    "phploc/phploc": "*",
    "sebastian/phpcpd": "*",
    "mayflower/php-codebrowser": "~1.1",
    "phing/phing": "^2.0",
    "sclable/xml-lint": "^0.2"
  },
  "autoload": {
    "psr-4": {
      "Redfox\\Autoload\\": "src",
      "Redfox\\Autoload\\Test\\": "test",
      "Redfox\\Autoload\\Test\\Unit\\testData\\": "test/unit/_testdata"
    },
    "exclude-from-classmap": [
      "/test/"
    ]
  }
}